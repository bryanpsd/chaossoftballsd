{
	"name": "chaos",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"start": "pnpm dev",
		"dev": "astro dev --open",
		"build": "astro build",
    "check": "astro check",
		"preview": "astro preview",
		"astro": "astro",
		"format": "pnpm format:prettier && pnpm format:biome",
		"format:biome": "biome format --write .",
		"format:prettier": "prettier --write \"**/*.astro\"",
		"lint": "pnpm lint:biome && pnpm lint:eslint",
		"lint:biome": "biome lint .",
		"lint:eslint": "eslint \"src/**/*.astro\"",
		"lint:fix": "pnpm lint:biome --write && pnpm lint:eslint --fix",
		"cf-export": "contentful space export --config src/services/contentful/export-config.json --management-token $CONTENTFUL_DELIVERY_TOKEN --space-id $CONTENTFUL_SPACE_ID",
		"cf-generate-types": "cf-content-types-generator src/services/contentful/export.json --v10 --typeguard --out src/types/contentful"
	},
	"dependencies": {
		"@astrojs/check": "^0.9.6",
		"@astrojs/netlify": "^6.6.0",
		"@astrojs/partytown": "^2.1.4",
		"@astrojs/react": "^4.4.2",
		"@astrojs/sitemap": "^3.6.0",
		"@contentful/rich-text-html-renderer": "^17.1.3",
		"@contentful/rich-text-react-renderer": "^16.1.3",
		"@contentful/rich-text-types": "^17.2.2",
		"@google-cloud/recaptcha-enterprise": "^6.3.0",
		"@radix-ui/react-accordion": "^1.2.12",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-navigation-menu": "^1.2.14",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@vanilla-extract/css": "^1.17.4",
		"@vanilla-extract/recipes": "^0.5.7",
		"@vanilla-extract/sprinkles": "^1.6.5",
		"astro": "^5.15.8",
		"astro-icon": "^1.1.5",
		"astro-robots-txt": "^1.0.0",
		"contentful": "^11.7.18",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.62.0",
		"react-icons": "^5.5.0",
		"react-toastify": "^11.0.5",
		"recaptcha-v3": "^1.11.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/node": "^24.5.2",
		"@typescript-eslint/eslint-plugin": "^8.47.0",
		"@typescript-eslint/parser": "^8.47.0",
		"@vanilla-extract/vite-plugin": "^5.1.1",
		"axe-core": "^4.10.3",
		"cf-content-types-generator": "^2.16.0",
		"eslint": "^9.39.1",
		"eslint-plugin-astro": "^1.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-astro": "^0.14.1",
		"typescript": "^5.8.3"
	}
}
